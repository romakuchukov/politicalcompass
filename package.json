{
  "name": "d3-political-compass",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": "14.x",
    "npm": "6.14"
  },
  "scripts": {
    "start": "rollup -c prod.rollup.config.js",
    "scss": "sass --watch style.scss:dist/style.css",
    "build": "node-sass --output-style compressed style.scss > dist/style.css & rollup -c prod.rollup.config.js",
    "watch": "rollup -c -w",
    "sync": "browser-sync --serveStatic . --files dist -w",
    "php": "php -S localhost:8000",
    "dev": "yarn run scss & yarn run watch & yarn run sync"
  },
  "dependencies": {
    "d3-selection": "^2.0.0"
  },
  "devDependencies": {
    "rollup": "^2.28.1",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-arrow-functions": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "node-sass": "^4.14.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-watch": "^4.3.1"
  }
}
